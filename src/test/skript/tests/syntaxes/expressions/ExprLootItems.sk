test "loot items":

	set {_context} to a new loot context at spawn of "world"
	set {_items::*} to loot items of loot table "minecraft:chests/simple_dungeon" with context {_context}

	assert {_items::*} is set with "loot items not set"

	set {_items::*} to loot items of loot table "minecraft:entities/pig" with context {_context}
	assert {_items::*} is not set with "loot items set"
